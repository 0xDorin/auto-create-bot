# Network Configuration
NETWORK_MODE=mainnet

# Mainnet
MAINNET_RPC_URL=https://your-monad-mainnet-rpc.com
MAINNET_CHAIN_ID=10200

# Wallet Configuration
MNEMONIC=your twelve word mnemonic phrase here for wallet derivation

# Bot Configuration
NUM_WALLETS=10
TOTAL_TOKENS_TO_CREATE=100
DURATION_HOURS=4

# Execution Mode
# - sequential: Create tokens one by one with fixed delays
# - parallel: Create all tokens in parallel with random delays
EXECUTION_MODE=parallel
# Randomness factor for delays (0.0 = no randomness, 1.0 = Â±100% random)
# Example: If average delay is 1 min and randomness is 0.5, actual delay will be 30s-90s
DELAY_RANDOMNESS=0.5

# Initial Buy Configuration (in MON)
INITIAL_BUY_AMOUNT=0.1
SELL_PERCENTAGE=100

# Gas Configuration
GAS_LIMIT=500000
MAX_FEE_PER_GAS=2
MAX_PRIORITY_FEE_PER_GAS=1

# Metadata API
# Token list fetch (always mainnet - more tokens available)
TOKEN_LIST_API_BASE_URL=https://api.nad.fun

# Metadata upload (use network-specific API)
MAINNET_METADATA_API_BASE_URL=https://api.nad.fun

# Metadata fetch settings
METADATA_START_PAGE=1
METADATA_LIMIT_PER_PAGE=100

# Metadata preparation mode
# - upload: Download images from mainnet and re-upload to current network
# - reuse: Reuse existing metadata URIs (not implemented yet)
METADATA_MODE=upload

# Funding (MON amount to send to each wallet)
WALLET_FUNDING_AMOUNT=50

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY_MS=5000
